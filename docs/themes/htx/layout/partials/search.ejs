  html:not(.js-enabled .AlgoliaSearch  {
    display: none;
  }

  .ds-dropdown-menu) {
    background-color: white;
    border: 1px black solid;
    /* top: calc(100% + 4px)!important; Needed to overwrite Algoliaâ€™s default style */
    top: calc(100% + 7.5px)!important;
    padding: 1em;
