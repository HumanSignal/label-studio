<script type="text/javascript" src="https://cdn.jsdelivr.net/npm/docsearch.js@2/dist/cdn/docsearch.min.js" defer></script>
<script>

window.addEventListener('load', (event) => {
  window.docsearch({
    container: '#docsearch-input',
    inputSelector: '#docsearch-input',
    appId: 'HELLEDAKPT',
    apiKey: '1d0410ef855a968fbc40669df1c4a73e',
    indexName: 'labelstud', // it does not change
  });

  const searchInput = document.querySelector("#docsearch-input");
  const handleFocusSearch = (e) => {
    const notCMDChromeFirefox = e.keyCode !== 91 && e.keyCode !== 93 && e.keyCode !== 224 && !e.metaKey;
    if (document.activeElement.localName === 'body' && e.code !== "Space" && e.code !== "MetaLeft" && !e.altKey && !e.shiftKey && !e.ctrlKey && notCMDChromeFirefox) {
      searchInput.focus();
    }
  }

  window.addEventListener("keydown", handleFocusSearch);

  
});
  

</script>