          <script>
          fetch('https://api.github.com/repos/heartexlabs/label-studio/contributors')
          .then(res => res.json())
          .then(users => {
            const sample = document.querySelector('.elem__community')
            if (!sample) return
            const container = sample.parentNode
            container.removeChild(sample)
            // shuffle them
            users.sort(() => Math.random() - 0.5).forEach(user => {
              if (user.type !== "User") return
              const el = sample.cloneNode(true)
              el.querySelector('img.community__team').src = user.avatar_url
              el.querySelector('.community__info p').textContent = user.name || ""
              el.querySelector('.community__info a').href = user.html_url
              el.querySelector('.community__info a').innerHTML = "<span>@</span> " + user.login
