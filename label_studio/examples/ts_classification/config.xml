<!-- {"title": "Time Series classification", "category": "time-series", "complexity": "basic", "order": "!"} -->
<View>
    <Header value="Time Series Classification" />

    <TimeSeries name="ts" timeColumn="time"
                value="$ts" valueType="url">
        <Channel column="first_column"
                 strokeColor="#1f77b4" />
    </TimeSeries>

    <Choices name="pattern" toName="ts" showInline="true">
        <Choice value="Growth"/>
        <Choice value="Decay"/>
    </Choices>
</View>
